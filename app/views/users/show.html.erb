
<h1><%= @user.email %></h1>
<h1><%= @user.username %></h1>
<h1><%= @user.role %></h1>
<h1><%= @user.id %></h1>
<div>
  <h1>Wikis you are the creator of</h1>
    <% @wikiowner.each do |wikiowner| %>
      <ul>
        <li><%= link_to "#{wikiowner.title}", wiki_path(wikiowner), class: "ui-button-text button_text" %></li>
      </ul>
    <% end %>
</div>

<div>
  <h1>Wikis you are a collaborator on</h1>
    <% @user.collaborators.each do |wikicollaborator| %>
        <ul>
          <li><%= link_to "#{wikicollaborator.wiki.title}", wiki_path(wikicollaborator), class: "ui-button-text button_text" %></li>
        </ul>
    <% end %>
</div>

<p><a class="btn btn-success btn-lg" href="/users/edit" role="button">Edit Profile</a></p>


<h1><%= link_to "Upgrade Account", new_charge_path %></h1>

<h3><%= link_to "Downgrade Account", user_downgrade_path, method: :put, data: { confirm: 'Are you sure you want to downgrade your account? This will make all your private wikis now Public.' } %></h3>

<h1><%= flash[:notice] %></h1>
